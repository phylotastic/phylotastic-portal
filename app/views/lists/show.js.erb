$('#new-list-wrapper > div').removeClass("panel-primary");
$('#new-list-wrapper > div').addClass("panel-default");

<% if @from_service %>

<% else %>
  $('#list-content').html(
    "<%= escape_javascript(render partial: 'lists/content', 
                                  locals: { :list            => @list,
                                            :list_name       => @list_name,
                                            :names           => @names, 
                                            :unmatched_names => @unmatched_names }) %>"
  );
<% end %>

$('#list-actions').html(
  "<%= escape_javascript(render partial: 'lists/actions', 
                               locals: { :list         => @list, 
                                         :is_private   => @is_private,
                                         :from_service => @from_service }) %>"
);

$('#list-action-seperator').removeClass("hide");